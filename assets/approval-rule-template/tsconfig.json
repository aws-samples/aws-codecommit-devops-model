{
  "compilerOptions": {
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowJs": true,
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es5",
      "es2015.promise"
    ],
    "outDir": "./"
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ]
}